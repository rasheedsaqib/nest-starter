{
  "name": "nest-starter",
  "version": "0.0.1",
  "description": "",
  "author": "Saqib Rasheed",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format:eslint": "eslint src --ext .ts --fix",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format": "yarn run format:eslint && yarn run format:prettier",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:type": "tsc",
    "lint": "yarn run lint:lint && yarn run lint:type",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config src/e2e/jest-e2e.json",
    "postinstall": "npx husky install"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mongoose": "^7.6.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@faker-js/faker": "^8.2.0",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.20",
    "@types/jest": "29.5.6",
    "@types/node": "20.8.8",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "37.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.2.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.21.0",
    "eslint-plugin-unicorn": "48.0.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "mongodb-memory-server": "^9.0.1",
    "prettier": "3.0.3",
    "source-map-support": "^0.5.20",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  }
}
