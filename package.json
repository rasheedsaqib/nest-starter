{
  "name": "nest-starter",
  "version": "0.0.1",
  "description": "",
  "author": "Saqib Rasheed",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format:eslint": "eslint src --ext .ts --fix",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format": "yarn run format:eslint && yarn run format:prettier",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:type": "tsc",
    "lint": "yarn run lint:lint && yarn run lint:type",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config src/e2e/jest-e2e.json",
    "postinstall": "npx husky install"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/platform-express": "^9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "mongoose": "^8.2.4",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.29",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "37.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-sonarjs": "0.25.0",
    "eslint-plugin-unicorn": "51.0.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "mongodb-memory-server": "^9.1.4",
    "prettier": "3.2.5",
    "source-map-support": "^0.5.20",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.2"
  }
}
